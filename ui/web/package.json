{
  "name": "toth-web-ui",
  "version": "0.0.1",
  "description": "Default UI for toth",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:prod": "webpack",
    "build:dev": "webpack --env dev --progress --profile --colors",
    "build:watch": "webpack --env dev --progress --profile --colors --watch",
    "dev:listen": "webpack-dev-server --open",
    "lint": "eslint --max-warnings=0 --ext .js,.jsx,.vue,.ts,.tsx src"
  },
  "author": "Olivier Ory",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.1",
    "line-awesome": "^1.3.0",
    "lodash": "^4.17.15",
    "vue": "^2.6.11",
    "vue-class-component": "^7.1.0",
    "vue-i18n": "^8.15.3",
    "vue-property-decorator": "^8.3.0",
    "vue-router": "^3.1.5",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.15.0",
    "@typescript-eslint/parser": "^2.15.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.4.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-vue": "^6.1.2",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.13.0",
    "prettier": "1.19.1",
    "sass-loader": "^8.0.0",
    "standard": "^14.3.1",
    "style-loader": "^1.1.2",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.4",
    "url-loader": "^3.0.0",
    "vue-loader": "^15.8.3",
    "vue-svg-loader": "^0.16.0",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.11.0"
  },
  "eslintConfig": {
    "parser": "vue-eslint-parser",
    "parserOptions": {
      "ecmaVersion": 11,
      "sourceType": "module",
      "parser": "@typescript-eslint/parser"
    },
    "env": {
      "browser": true,
      "es2020": true
    },
    "extends": [
      "plugin:vue/essential",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:prettier/recommended",
      "standard",
      "prettier"
    ],
    "plugins": [
      "vue",
      "@typescript-eslint",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error"
    }
  }
}
